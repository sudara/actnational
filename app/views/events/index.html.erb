<p id="notice"><%= notice %></p>

<h1>Events</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Category</th>
      <th>Region</th>
      <th>Topic</th>
      <th>Start</th>
      <th>End</th>
      <th>Organizer</th>
      <th>Contact email</th>
      <th>Contact phone</th>
      <th>Website</th>
      <th>Address</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th>Location details</th>
      <th>test</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.name %></td>
        <td><%= event.description %></td>
        <td><%= event.category %></td>
        <td><%= event.region %></td>
        <td><%= event.topic %></td>
        <td><%= event.start %></td>
        <td><%= event.end %></td>
        <td><%= event.organizer %></td>
        <td><%= event.contact_email %></td>
        <td><%= event.contact_phone %></td>
        <td><%= event.website %></td>
        <td><%= event.address %></td>
        <td><%= event.city %></td>
        <td><%= event.state %></td>
        <td><%= event.zip %></td>
        <td><%= event.location_details %></td>
        <td><%= link_to 'Show', event %></td>
        <td><%= link_to 'Edit', edit_event_path(event) %></td>
        <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event', new_event_path %>
